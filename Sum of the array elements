#include <stdio.h>
void Arrayfun();
int main() {
   
    Arrayfun();
    return 0;
}

void Arrayfun(){
    int n,i=0,a[100],sum=0;
printf("Enter the array limit") ;   
scanf("%d",&n);
printf("Enter the array elements");
while(i<=n-1){
    scanf("%d",&a[i]);
    i++;
}
 
 for(i=0;i<n;i++){
    sum=sum+a[i];
 }
 printf("The sum of the  array elements is %d ",sum);
    
}
